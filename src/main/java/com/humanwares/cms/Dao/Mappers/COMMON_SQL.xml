<?xml version="1.0" encoding= "UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="COMMON">


	<select id="select_max_idx" parameterType="java.util.HashMap" resultType="java.lang.Integer">
		SELECT
			MAX(idx)
		FROM
			${ tb_name }
	</select>



</mapper>